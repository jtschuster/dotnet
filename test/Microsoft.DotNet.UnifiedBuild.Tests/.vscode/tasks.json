{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "test",
            "type": "process",
            "command": "dotnet",
            "args": [
                "test",
                "${workspaceFolder}/Microsoft.DotNet.UnifiedBuild.Tests.csproj",
            ],
            "options": {
                "env": {
                    // "VSTEST_HOST_DEBUG": "1",
                    "UNIFIED_BUILD_VALIDATION_SDK_TARBALL_PATH": "D:\\dn\\artifacts\\assets\\Release\\dotnet-sdk-9.0.100-preview.3.24164.1-win-x64.zip",
                    "UNIFIED_BUILD_VALIDATION_MSFT_SDK_TARBALL_PATH": "C:\\Users\\jschuster\\Downloads\\dotnet-sdk-9.0.100-preview.3.24164.5-win-x64.zip",
                    "UNIFIED_BUILD_VALIDATION_TARGET_RID": "win-x64",
                    "UNIFIED_BUILD_VALIDATION_PORTABLE_RID": "win-x64",
                    "UNIFIED_BUILD_VALIDATION_DOTNET_DIR": "C:\\Users\\jschuster\\AppData\\Local\\dnvm\\dn\\",
                    "UNIFIED_BUILD_VALIDATION_WARN_SDK_CONTENT_DIFFS": "true"
                }
            },
            "group": "test",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Microsoft.DotNet.UnifiedBuild.Tests.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Microsoft.DotNet.UnifiedBuild.Tests.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary;ForceNoAlign"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "--project",
                "${workspaceFolder}/Microsoft.DotNet.UnifiedBuild.Tests.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}